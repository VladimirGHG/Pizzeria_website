<+>{% extends 'pizzas/home.html' %}\r\n\r\n{% block content %}\r\n<center>\r\n    {% if searched_val and length_of_search > 3%}\r\n    <h1>Results for {{searched_val}}</h1>\r\n    <br/>\r\n    {% for pizza in filtered %}\r\n    <div class=\"col-sm-4\">\r\n        <div class=\"panel panel-primary\">\r\n            <div class=\"panel-heading\">\r\n                <a href=\"{% url 'pizza_detail' pizza.id %}\" style=\"color: white;\">{{ pizza.name }}</a>\r\n            </div>\r\n            <div class=\"panel-body\">\r\n                {% if pizza.image %}\r\n                {% load static %}\r\n                <img src=\"{{ pizza.image.url }}\"\r\n                     alt=\"{{ pizza.name }}\"\r\n                     style=\"width:100%\">\r\n                {% else %}\r\n                <img alt=\"Image\" class=\"img-responsive\" src=\"https://placehold.it/150x80?text=IMAGE\"\r\n                     style=\"width:100%\">\r\n                {% endif %}\r\n            </div>\r\n            <div class=\"panel-footer\">\r\n                Price: {{ pizza.price }} AMD\r\n                <div class=\"text-right\">\r\n                    Score: {{ pizza.score }}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    {% endfor %}\r\n    {% else %}\r\n    <h1>Nothing was found!</h1>\r\n    {% endif %}\r\n</center>\r\n{% endblock %}
